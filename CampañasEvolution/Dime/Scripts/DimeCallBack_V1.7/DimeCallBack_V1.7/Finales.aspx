    Protected Sub Page_Load(ByVal sender As Object, ByVal e As System.EventArgs) Handles Me.Load
        Dim Ya_iniciado As String = ""
        Dim idTransaccionCallBack As String = ""
        Dim telefonoCallBack As String = ""
        Dim fechaCallBack As String = ""
        Gestion.Transaccion.KeyValueCollection.GetValue("Ya_iniciado_A" + Gestion.Transaccion.IdTransaccion.ToString(), Ya_iniciado)
        If String.IsNullOrWhiteSpace(Ya_iniciado) Then
            'Si es la primera vez que pasamos por aquí
            idTransaccionCallBack = Request.QueryString("Idtransaction").ToString()
            telefonoCallBack = Request.QueryString("telefono").ToString()
            fechaCallBack = Request.QueryString("fecha").ToString()
            TextBox1.Text = telefonoCallBack
            TextBox2.Text = fechaCallBack
            Gestion.Transaccion.KeyValueCollection.SetValue("idTransaccionCallBack_A" + Gestion.Transaccion.IdTransaccion.ToString(), idTransaccionCallBack)
            Gestion.Transaccion.KeyValueCollection.SetValue("telefonoCallBack_A" + Gestion.Transaccion.IdTransaccion.ToString(), telefonoCallBack)
            Gestion.Transaccion.KeyValueCollection.SetValue("fechaCallBack_A" + Gestion.Transaccion.IdTransaccion.ToString(), fechaCallBack)
            Gestion.Transaccion.KeyValueCollection.SetValue("Ya_iniciado_A" + Gestion.Transaccion.IdTransaccion.ToString(), "true")
        Else
            Gestion.Transaccion.KeyValueCollection.GetValue("idTransaccionCallBack_A" + Gestion.Transaccion.IdTransaccion.ToString(), idTransaccionCallBack)
            Gestion.Transaccion.KeyValueCollection.GetValue("telefonoCallBack_A" + Gestion.Transaccion.IdTransaccion.ToString(), telefonoCallBack)
            Gestion.Transaccion.KeyValueCollection.GetValue("fechaCallBack_A" + Gestion.Transaccion.IdTransaccion.ToString(), fechaCallBack)
            TextBox1.Text = telefonoCallBack
            TextBox2.Text = fechaCallBack
        End If
        
    End Sub


